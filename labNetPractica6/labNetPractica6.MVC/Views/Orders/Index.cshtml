@model List<labNetPractica3.EF.Logic.Order.OrderDto>

@{
    ViewBag.Title = "Order Page";
}

<h1>Tabla de Ordenes</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Ship</th>
            <th scope="col">City</th>
            <th scope="col">Region</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <th scope="row">@order.Id</th>
                <td>@order.Ship</td>
                <td>@order.City</td>
                <td>@order.Region</td>
            </tr>
        }
    </tbody>
</table>
